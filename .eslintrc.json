{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es6": false,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "error",
        "strict": ["error", "global"],
        "curly": "warn",
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "import/extensions": [
            "error",
            "ignorePackages",
            {
                "js": "never",
                "jsx": "never",
                "ts": "never",
                "tsx": "never"
            }
        ]
    }
}
